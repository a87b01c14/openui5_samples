<mvc:View
		controllerName="sap.f.sample.ShellBarWithMenuButton.ShellBarWithMenuButton"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.f"
		xmlns:m="sap.m"
		xmlns:layout= "sap.ui.layout"
		height="100%">
		<ShellBar
				id="sapFShellBarSample"
				title="Application Title"
				secondTitle="Short description"
				homeIcon="https://www.sap.com/dam/application/shared/logos/sap-logo-svg.svg.adapt.svg/1493030643828.svg"
				showCopilot="true"
				showSearch="true"
				showMenuButton="true"
				showNavButton="true"
				showNotifications="true"
				class="sapFShellBarFCLFPHeader"
				notificationsNumber="2">
				<profile>
					<m:Avatar initials="UI"/>
				</profile>
		</ShellBar>
</mvc:View>